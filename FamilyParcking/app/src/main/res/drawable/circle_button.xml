    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="oval">
            <solid android:color="@color/blue" />
        </shape>
    </item>
    <item android:right="4dp" android:left="4dp" android:top="4dp" android:bottom="4dp">
